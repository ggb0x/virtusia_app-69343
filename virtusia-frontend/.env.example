# Configurações do Virtusia Frontend

# Nome da aplicação
VITE_APP_NAME=Virtusia

# Versão da aplicação
VITE_APP_VERSION=1.0.0

# URL da API backend
VITE_API_BASE_URL=https://virtusia-backend.onrender.com/api

# URL da API backend em produção
# VITE_API_BASE_URL=https://virtusia-backend.onrender.com/api

# Configurações de autenticação
VITE_JWT_SECRET=jwt-secret-key-virtusia

# Configurações do Firebase (para notificações push)
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456
VITE_FIREBASE_VAPID_KEY=your-vapid-key

# Configurações de analytics (opcional)
VITE_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID

# Configurações de desenvolvimento
VITE_DEV_MODE=true

# Configurações de debug
VITE_DEBUG_MODE=false

